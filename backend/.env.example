# API Keys - Required for testing
OPENAI_API_KEY=your_openai_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# Additional API Keys (Optional but used in various parts)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CX=your_google_cx_here
SERP_API_KEY=test_serp_key
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true
WORKERS=4

# CORS Configuration (matching Settings class)
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173,http://localhost:3002

# Database Configuration
DATABASE_URL=sqlite+aiosqlite:///./strands_swarm.db

# Security
SECRET_KEY=your_secret_key_change_in_production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Model Configuration
DEFAULT_MODEL_ID=gpt-4o
DEFAULT_MAX_TOKENS=16000
STRANDS_MAX_TOKENS=16000
MAX_HANDOFFS=20
MAX_ITERATIONS=20
EXECUTION_TIMEOUT=900.0
NODE_TIMEOUT=300.0

# Enhanced Swarm Service
USE_ENHANCED_SWARM=true

# WebSocket Settings
WS_MESSAGE_QUEUE_SIZE=100
WS_HEARTBEAT_INTERVAL=30

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Redis Settings (Optional)
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_TTL=3600
REDIS_MAX_CONNECTIONS=50

# AWS Settings for Bedrock (Optional)
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=

# Task Queue (Optional - can be enabled with Redis)
CELERY_BROKER_URL=
CELERY_RESULT_BACKEND=